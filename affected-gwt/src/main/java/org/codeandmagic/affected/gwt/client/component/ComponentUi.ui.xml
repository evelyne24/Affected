<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.component {
			width: 200px;
			height: 200px;
			border: 1px solid #306084;
			background-color: #4da4e2;
		}
		.prettyName {
			font-weight: bold;
		}
	</ui:style>
		
	<g:HTMLPanel styleName="{style.component}">
		<span class="{style.prettyName}" ui:field="prettyName" />
	</g:HTMLPanel>
</ui:UiBinder> 
